<body>
  <div id=headerWrapper>
    <h1 id=headerWrapper__header>UK University Ranking Data Comparison App</h1>
  </div>
  <div id=descWrapper>
    <p id=descWrapper__p>Select a university ranking dataset to view or select a comparison overview of all three league
      tables</p>
  </div>

  <div id=cards>
    <div class=card>
      <div class=card__imgWrapper>
        <a href="/guardian/overview"> <img src="res/theGuardianLogo.png" alt="TheGuardian" width=200px;> </a>
      </div>
      <a href="/guardian/overview">
        <button id=guardianButton>
          <p class=cards__card__text>The Guardian Ranking Data</p>
        </button>
      </a>

    </div>
    <div class=card>
      <div class=card__imgWrapper>
        <a href="/timesPCP"> <img src="res/theSundayTimesLogo.png" alt="TheSundayTimes" width=200px;> </a>
      </div>
      <a href="/timesPCP">
        <button id=sundayTimesButton>
          <p class=cards__card__text>The Sunday Times Ranking Data</p>
        </button>
      </a>
    </div>
    <div class=card>
      <div id=CUGWrapper class=card__imgWrapper>
        <a href="/completeUniPCP"> <img src="res/CUGLogo.png" alt="TheSundayTimes" width=200px;> </a>
      </div>
      <a href="/completeUniPCP">
        <button id=CUGButton>
          <p class=cards__card__text>The Complete University Guide</p>
        </button>
      </a>
    </div>
    <div class=card>
      <div class=card__imgWrapper>
        <a href="/comparison"> <img src="res/comparisonLogo.png" alt="TheCompleteUniversityGuide" width=200px;> </a>
      </div>
      <a href="/comparison">
        <button>
          <p class=cards__card__text>Comparison of University Guides</p>
        </button>
      </a>
    </div>
  </div>

